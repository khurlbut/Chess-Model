package strategy;

public class ChessScore implements BoardGrade {

    private int material;
    private int territory;
    private int position;
    private int collaborator;

    public ChessScore(int material, int territory, int position, int collaborator) {
        this.material = material;
        this.territory = territory;
        this.position = position;
        this.collaborator = collaborator;
    }

    @Override
    public Integer materialGrade() {
        return material;
    }

    @Override
    public Integer finalGrade() {
        return material;
    }

    @Override
    public Integer territoryGrade() {
        return territory;
    }

    @Override
    public Integer positionGrade() {
        return position;
    }

    @Override
    public Integer collaborationGrade() {
        return collaborator;
    }

    @Override
    public BoardGrade lowerGrade(BoardGrade lowerScore) {
        // TODO Auto-generated method stub
        return null;
    }

}
