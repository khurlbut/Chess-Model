package strategy;

public class ChessScore implements PositionAnalysis {

    private int material;
    private int territory;
    private int position;
    private int collaborator;

    public ChessScore(int material, int territory, int position, int collaborator) {
        this.material = material;
        this.territory = territory;
        this.position = position;
        this.collaborator = collaborator;
    }

    @Override
    public Integer material() {
        return material;
    }

    @Override
    public Integer totalScore() {
        return material;
    }

    @Override
    public Integer territoryGrade() {
        return territory;
    }

    @Override
    public Integer positionGrade() {
        return position;
    }

    @Override
    public Integer collaboratorGrade() {
        return collaborator;
    }

    @Override
    public PositionAnalysis lowestScore(PositionAnalysis lowerScore) {
        // TODO Auto-generated method stub
        return null;
    }

}
